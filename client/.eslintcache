Items\n            .filter((x) => Object\n                .values(x)\n                .some((value) => (value + '').toLowerCase().includes(searchChar.toLowerCase())))\n        : cardItems;\n\n    return (\n        <section className=\"shop mainbg\">\n            <div className=\"container\">\n                <div className=\"shop__search search shd\">\n                    <span className=\"search__count\">\n                        Товара найдено:\n                        <span className=\"search__number\">\n                            {visibleCards.length >= 0 && searchChar !== null\n                                ? visibleCards.length\n                                : cardItems.length}\n                        </span>\n                    </span>\n                    <SearchForm\n                        onChange={onChange} />\n                    <div className=\"search__sort\">\n                        Сортировать по: <span className=\"search__type\">Цене</span>\n                    </div>\n                </div>\n                <div className=\"shop__content\">\n                    <div className=\"shop__column shop__column_left\">\n                        <div className=\"shop__category category shd\">\n                            <span className=\"category__title\">Выберите тип товара</span>\n                            <ul className=\"category__list\">\n                                {items\n                                    && items.map((CategoryType) =>\n                                        <CategoryItem\n                                            {...CategoryType}\n                                            activeItem={activeItem}\n                                            key={CategoryType.id} />)}\n                            </ul>\n                        </div>\n                    </div>\n                    <div className=\"shop__column shop__column_right\">\n                        <div className=\"shop__cardbox\">\n                            {!isLoading\n                                ? visibleCards.map((card) =>\n                                    <Card\n                                        {...card}\n                                        key={card.id} />)\n                                : loaderItems.map((loader) => <Loader key={loader.id} />)}\n                        </div>\n                    </div>\n                </div>\n\n            </div >\n        </section>\n    )\n})\n\nexport default Shop;\n\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\PreviewItem.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\CartItem.jsx",["204","205"],"import React, { useState, useEffect } from 'react';\nimport { useSelector } from 'react-redux';\nimport { ReactComponent as DeleteItem } from '../assets/images/svg/clear-single.svg';\nimport { ReactComponent as Plus } from '../assets/images/svg/plus.svg';\nimport { ReactComponent as Minus } from '../assets/images/svg/minus.svg';\nimport { useDispatch } from 'react-redux';\nimport { minusCartItem, setCartItems } from '../redux/actions/cart';\nimport { setCancelId } from '../redux/actions/modal';\n\nfunction CartItem({ id, title, img, size, material, price, description, dataForKey, handlerCancelModalShow }) {\n\n    const dispatch = useDispatch();\n\n    const [disableMinus, setDisableMinus] = useState(false);\n\n    const { countsIdItems } = useSelector((state) => ({\n        countsIdItems: state.cart.countsIdItems,\n    }))\n\n    const handlerCartItem = () => {\n        const obj = {\n            id,\n            title,\n            img,\n            description,\n            size,\n            material,\n            price,\n            dataForKey\n        }\n        dispatch(setCartItems(obj))\n    }\n\n    const onSetDisableMinus = () => {\n        if (countsIdItems[id] === 1) {\n            setDisableMinus(true)\n        } else {\n            setDisableMinus(false)\n        }\n    }\n\n    const handlerMinusItem = () => {\n        if (countsIdItems[id] !== 1) {\n            dispatch(minusCartItem(id))\n        }\n        onSetDisableMinus()\n    }\n\n    const handlerCancel = () => {\n        handlerCancelModalShow()\n        dispatch(setCancelId(id))\n    }\n\n    useEffect(() => {\n        onSetDisableMinus()\n    }, [countsIdItems[id]])\n\n    const MinusButton = disableMinus ? \"item__button shd btn disabled reff\" : \"item__button shd btn reff\";\n\n    return (\n        <div className=\"cart__item item shd\">\n            <div className=\"item__column\">\n                <div className=\"item__imgbox\">\n                    <img className=\"item__img\" src={img} alt=\"\" />\n                </div>\n                <h2 className=\"item__name\">{title}</h2>\n            </div>\n            <div className=\"item__column\">\n                <span className=\"item__description\">{description}</span>\n            </div>\n            <div className=\"item__column\">\n                <div className=\"item__row\">\n                    <span className=\"item__size\">Размер:</span>\n                    <span className=\"item__size-count\">{size} см</span>\n                </div>\n                <div className=\"item__row\">\n                    <span className=\"item__material\">Материал:</span>\n                    <span className=\"item__material-value\">{material}</span>\n                </div>\n            </div>\n            <div className=\"item__column\">\n                <span className=\"item__text\">Цена:</span>\n                <span className=\"item__price\">{price} руб</span>\n            </div>\n            <div className=\"item__column\">\n                <div className=\"item__count\">\n                    <button\n                        className={MinusButton}>\n                        <Minus\n                            onClick={handlerMinusItem}\n                            className=\"item__minus\" />\n                    </button>\n                    <span className=\"item__count-value\">{countsIdItems[id]} шт</span>\n                    <button className=\"item__button item__button_green shd btn geff\">\n                        <Plus\n                            onClick={handlerCartItem}\n                            className=\"item__plus\"\n                        />\n                    </button>\n                </div>\n            </div>\n            <div className=\"item__column\">\n                <span className=\"item__text\">Стоимость:</span>\n                <span className=\"item__totalprice\">{price * countsIdItems[id]} руб</span>\n            </div>\n            <div className=\"item__column\">\n                <button onClick={handlerCancel} className=\"item__button shd btn reff\">\n                    <DeleteItem className=\"item__svg\" />\n                </button>\n            </div>\n\n        </div >\n    )\n}\n\nexport default CartItem\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\types.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\page.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\loader.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\categoryes.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\cards.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\search.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\Loader.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\CategoryItem.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\SearchForm.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\Card.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\reducers\\page.js",[],"C:\\Upload\\Projects\\Re
   })\r\n                .catch(e => {\r\n                    dispatch(setUser(false))\r\n                    dispatch(setAuth(false))\r\n                })\r\n        }\r\n    }, [])\r\n\r\n    return (\r\n        <div className=\"wrapper\">\r\n            <Header />\r\n            <Switch>\r\n                <Route exact path={HOME_ROUTE} component={Home} />\r\n                <Route exact path={CARD_PAGE_ROUTE + '/:id'} component={CardPage} />\r\n                {isAuth &&\r\n                    <>\r\n                        <Route exact path={CART_ROUTE} component={Cart} />\r\n                        {user.role === \"ADMIN\" &&\r\n                            < Route exact path={ADMIN_ROUTE} component={Admin} />}\r\n                    </>\r\n                }\r\n                <Redirect to={HOME_ROUTE} />\r\n            </Switch>\r\n            <Footer />\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default App;\r\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\pages\\Home.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\pages\\Cart.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\pages\\CardPage.jsx",["228","229"],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\reducers\\index.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\width.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\modal.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\cart.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\reducers\\loader.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\reducers\\categoryes.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\reducers\\cards.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\reducers\\modal.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-m
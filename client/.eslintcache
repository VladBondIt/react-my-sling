\n                                {brandItems && brandItems.map((item) =>\n                                    <BrandItem key={item.name} {...item} activeBrandItem={activeBrandItem} />\n                                )}\n                            </ul>\n                        </div>\n                        <div className=\"shop__cardbox\">\n                            {!isLoading\n                                ? visibleCards.map((card) =>\n                                    <Card\n                                        card={card}\n                                        key={card.id} />)\n                                : loaderItems.map((loader) => <Loader key={loader.id} />)}\n                        </div>\n                        {!searchChar &&\n                            <Pagination\n                                limit={limit}\n                                brandBar={brandBar}\n                                totalCount={totalCount}\n                            />}\n                    </div>\n                </div>\n            </div >\n        </section>\n    )\n})\n\nexport default Shop;\n\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\PreviewItem.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\CartItem.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\types.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\page.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\loader.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\categoryes.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\cards.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\redux\\actions\\search.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\client\\src\\components\\Loader.jsx",[],"C:\\Upload\\Projects\\ReactBeg
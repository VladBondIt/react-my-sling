;\nimport CategoryItem from './CategoryItem';\nimport SearchForm from './SearchForm';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { setCategoryes } from '../redux/actions/categoryes';\nimport { fetchedCards } from '../redux/actions/cards';\nimport Loader from './Loader';\nimport { setLoading } from '../redux/actions/loader';\nimport { setSearchChar } from '../redux/actions/search';\nimport FetchService from '../services/fetchService';\n\n\nconst Shop = memo(function Shop() {\n    const dispatch = useDispatch();\n\n    const { items, activeItem, cardItems, loaderItems, isLoading, searchChar, } = useSelector((state) => ({\n        items: state.categoryes.items,\n        activeItem: state.categoryes.activeItem,\n        cardItems: state.cards.cardItems,\n        loaderItems: state.loader.loaderItems,\n        isLoading: state.loader.isLoading,\n        searchChar: state.search.searchChar,\n    }))\n\n    const fetchItems = () => {\n        if (items.length === 0) {\n            FetchService.getCategoryes('slings').then((res) => {\n                dispatch(setCategoryes(res));\n            })\n        }\n        dispatch(setLoading(false))\n        if (cardItems.length === 0) {\n            dispatch(fetchedCards())\n        }\n    }\n\n    useEffect(() => {\n        fetchItems();\n    }, [])\n\n    const onChange = (e) => {\n        dispatch(setSearchChar(e.target.value))\n    }\n\n    const visibleCards = searchChar !== null\n        ? cardItems\n            .filter((x) => Object\n                .values(x)\n                .some((value) => (value + '').toLowerCase().includes(searchChar.toLowerCase())))\n        : cardItems;\n\n    return (\n        <section className=\"shop mainbg\">\n            <div className=\"container\">\n                <div className=\"shop__search search shd\">\n                    <span className=\"search__count\">\n                        Товара найдено:\n                        <span className=\"search__number\">\n                            {visibleCards.length >= 0 && searchChar !== null\n                                ? visibleCards.length\n                                : cardItems.length}\n                        </span>\n                    </span>\n                    <SearchForm\n                        onChange={onChange} />\n                    <div className=\"search__sort\">\n                        Сортировать по: <span className=\"search__type\">Цене</span>\n                    </div>\n                </div>\n                <div className=\"shop__content\">\n                    <div className=\"shop__column shop__column_left\">\n                        <div className=\"shop__category category shd\">\n                            <span className=\"category__title\">Выберите тип товара</span>\n                            <ul className=\"category__list\">\n                                {items\n                                    && items.map((CategoryType) =>\n                                        <CategoryItem\n                                            {...CategoryType}\n                                            activeItem={activeItem}\n                                            key={CategoryType.id} />)}\n                            </ul>\n                        </div>\n                    </div>\n                    <div className=\"shop__column shop__column_right\">\n                        <div className=\"shop__cardbox\">\n                            {!isLoading\n                                ? visibleCards.map((card) =>\n                                    <Card\n                                        {...card}\n                                        key={card.id} />)\n                                : loaderItems.map((loader) => <Loader key={loader.id} />)}\n                        </div>\n                    </div>\n                </div>\n\n            </div >\n        </section>\n    )\n})\n\nexport default Shop;\n\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\Card.jsx",["166","167"],"import React from 'react';\r\nimport ForCard0 from '../assets/images/card/for_card-cut.png';\r\nimport ForCard1 from '../assets/images/card/sling-w-ring.jpg';\r\nimport ForCard2 from '../assets/images/card/sling-backpack.jpg';\r\nimport { useDispatch } from 'react-redux';\r\nimport { setCartItems } from '../redux/actions/cart';\r\nimport { setModalShow, setModalType, setPreviewObj } from '../redux/actions/modal';\r\nimport { Link } from 'react-router-dom';\r\n\r\nfunction Card({ title, description, material, size, price, oldprice, img, id }) {\r\n    const dispatch = useDispatch();\r\n\r\n\r\n    switch (img) {\r\n        case 0:\r\n            img = ForCard0\r\n            break;\r\n        case 1:\r\n            img = ForCard1\r\n            break;\r\n        case 2:\r\n            img = ForCard2\r\n            break;\r\n\r\n        default:\r\n            break;\r\n    }\r\n    const obj = {\r\n        id,\r\n        title,\r\n        img,\r\n        description,\r\n        size,\r\n        material,\r\n        price,\r\n        oldprice,\r\n        dataForKey: Date.now().toString(16)\r\n    }\r\n\r\n    const handlerCardItem = () => {\r\n        dispatch(setCartItems(obj))\r\n    }\r\n\r\n    const handlerPreview = () => {\r\n        dispatch(setPreviewObj(obj))\r\n    }\r\n\r\n\r\n    return (\r\n        <div className=\"shop__card card shd\" >\r\n            <div className=\"card__imagebox\">\r\n                <img className=\"card__image\" src={img} alt={title} />\r\n                <div className=\"card__overlay overlay\">\r\n                    <Link to=\"/react-my-sling/cardpage\">\r\n                        <button\r\n                            onClick={handlerPreview}\r\n                            className=\"overlay__button btn eff\">Предпросмотр</button>\r\n                    </Link>\r\n                </div>\r\n            </div>\r\n            <div className=\"card__infobox\">\r\n                <div className=\"card__textbox\">\r\n                    <h2 className=\"card__title\">{title}</h2>\r\n                    <span className=\"card__subtitle\">{description}</span>\r\n                </div>\r\n                <div className=\"card__row\">\r\n                    <span className=\"card__size\">Размер:</span>\r\n                    <span className=\"card__size-count\">{size} см</span>\r\n                </div>\r\n                <div className=\"card__row\">\r\n                    <span className=\"card__material\">Материал:</span>\r\n                    <span className=\"card__material-value\">{material}</span>\r\n                </div>\r\n                <div className=\"card__row\">\r\n                    <div className=\"card__price price\">\r\n                        <span className=\"price__old\">{oldprice} руб</span>\r\n                        <span className=\"price__current\">{price} руб</span>\r\n                    </div>\r\n                    <button\r\n                        className=\"card__button add-button btn\"\r\n                        onClick={handlerCardItem}>\r\n                        Добавить\r\n                        </button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n\r\n    )\r\n}\r\n\r\nexport default Card\r\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\CategoryItem.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\SearchForm.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\store.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\types.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\actions\\categoryes.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\reducers\\index.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\reducers\\categoryes.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\reducers\\cards.js",[],
taForKey\n        }\n        dispatch(setCartItems(obj))\n    }\n\n    const onSetDisableMinus = () => {\n        if (countsIdItems[id] === 1) {\n            setDisableMinus(true)\n        } else {\n            setDisableMinus(false)\n        }\n    }\n\n    const handlerMinusItem = () => {\n        if (countsIdItems[id] !== 1) {\n            dispatch(minusCartItem(id))\n        }\n        onSetDisableMinus()\n    }\n\n    const handlerCancel = () => {\n        handlerCancelModalShow()\n        dispatch(setCancelId(id))\n    }\n\n    useEffect(() => {\n        onSetDisableMinus()\n    }, [countsIdItems[id]])\n\n    const MinusButton = disableMinus ? \"item__button shd btn disabled\" : \"item__button shd btn\";\n\n    return (\n        <div className=\"cart__item item shd\">\n            <div className=\"item__column\">\n                <div className=\"item__imgbox\">\n                    <img className=\"item__img\" src={img} alt=\"\" />\n                </div>\n                <h2 className=\"item__name\">{title}</h2>\n            </div>\n            <div className=\"item__column\">\n                <span className=\"item__description\">{description}</span>\n            </div>\n            <div className=\"item__column\">\n                <div className=\"item__row\">\n                    <span className=\"item__size\">Размер:</span>\n                    <span className=\"item__size-count\">{size} см</span>\n                </div>\n                <div className=\"item__row\">\n                    <span className=\"item__material\">Материал:</span>\n                    <span className=\"item__material-value\">{material}</span>\n                </div>\n            </div>\n            <div className=\"item__column\">\n                <span className=\"item__price\">{price} руб</span>\n            </div>\n            <div className=\"item__column\">\n                <div className=\"item__count\">\n                    <button\n                        className={MinusButton}>\n                        <Minus\n                            onClick={handlerMinusItem}\n                            className=\"item__minus\" />\n                    </button>\n                    <span className=\"item__count-value\">{countsIdItems[id]} шт</span>\n                    <button className=\"item__button item__button_green shd btn\">\n                        <Plus\n                            onClick={handlerCartItem}\n                            className=\"item__plus\"\n                        />\n                    </button>\n                </div>\n            </div>\n            <div className=\"item__column\">\n                <span className=\"item__totalprice\">{price * countsIdItems[id]} руб</span>\n            </div>\n            <div className=\"item__column\">\n                <button onClick={handlerCancel} className=\"item__button shd btn\">\n                    <DeleteItem className=\"item__svg\" />\n                </button>\n            </div>\n\n        </div >\n    )\n}\n\nexport default CartItem\n","C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\reducers\\cart.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\reducers\\modal.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\actions\\modal.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\actions\\search.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\redux\\reducers\\search.js",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\Modal.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\CartModal.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\OfferCallModal.jsx",[],"C:\\Upload\\Projects\\ReactBegin\\react-my-sling\\src\\components\\PreviewCardModal.jsx",["169"],"import React, { useState } from 'react';\r\nimport { useDispatch } from 'react-redux';\r\nimport { setCartItems } from '../redux/actions/cart';\r\nimport F